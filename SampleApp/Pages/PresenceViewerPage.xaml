<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SampleApp.Pages.PresenceViewerPage"
             xmlns:presence="clr-namespace:DittoPresenceViewer;assembly=DittoPresenceViewer"
             x:Name="self"
             Title="PresenceViewerPage">
    <presence:DittoPresenceViewer
        HorizontalOptions="Fill"
        VerticalOptions="Fill"
        Ditto="{Binding Ditto, Source={x:Reference self}}"/>
</ContentPage>
